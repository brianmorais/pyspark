{
	"name": "PySpark",
	"dockerComposeFile": [
		"docker-compose.yaml"
	],
	"service": "app",
	"workspaceFolder": "/workspace",
	"mounts": [
		"source=${localWorkspaceFolder},target=/workspace,type=bind,consistency=cached"
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python"
			]
		}
	}
}
